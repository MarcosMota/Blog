@model IEnumerable<Blog.Web.Models.Categorias.CategoriaModel>


 <!-- Blog Categories Well -->
<div class="well">
    <h4>Categorias</h4>
    <div class="row">
        <!-- /.col-lg-12 -->
            <div class="col-lg-12">
                <ul class="list-unstyled">
                    <li>
                        @foreach (var item in Model)
                        {
                            <a style="display: block;" href="#">@item.Descricao</a>
                        }
                        
                    </li>
                </ul>
            </div>
            <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
